{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 8 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": {
        "0": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "spc_dot_stat_data": {
            "type": "vcs",
            "url": "git@gitlab.com:pacific-community/drupal-modules/spc_dot_stat_data.git"
        },
        "spc_sdg_summary": {
            "type": "vcs",
            "url": "git@gitlab.com:pacific-community/drupal-modules/spc_sdg_summary.git"
        }
    },
    "require": {
        "composer/installers": "^1.2",
        "defuse/php-encryption": "^2.2",
        "drupal/admin_toolbar": " ^2.0",
        "drupal/backup_migrate": "^5.0@RC",
        "drupal/block_field": "^1.0",
        "drupal/ckeditor_bootstrap_grid": "^1.3",
        "drupal/color_field": "^2.4",
        "drupal/config_ignore": "^2.2",
        "drupal/core-composer-scaffold": "^9.1",
        "drupal/core-project-message": "^9.1",
        "drupal/core-recommended": "^9.1",
        "drupal/ctools": " ^3.2",
        "drupal/devel": "^4.0",
        "drupal/field_formatter_class": "^1.4",
        "drupal/google_analytics": "^2.5",
        "drupal/hotjar": "^2.0",
        "drupal/metatag": "^1.14",
        "drupal/page_manager": " ^4.0",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": " ^1.6",
        "drupal/redirect": "^1.6",
        "drupal/robotstxt": "^1.4",
        "drupal/svg_image": " ^1.9",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/views_infinite_scroll": "^1.7",
        "drupal/yearonly": "^9.0",
        "drush/drush": " ^10.2",
        "pacific_community/spc_dot_stat_data": "^1.0",
        "pacific_community/spc_sdg_summary": "^1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
